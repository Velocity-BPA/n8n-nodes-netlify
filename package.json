{
  "name": "n8n-nodes-netlify",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Netlify providing 12 resources and 60+ operations for site management, deployments, forms, functions, DNS, and A/B testing. Includes webhook triggers for deploy and form events.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-netlify.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-netlify#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-netlify/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "netlify",
    "jamstack",
    "deployment",
    "serverless",
    "web-hosting",
    "dns",
    "forms",
    "functions",
    "webhooks",
    "automation"
  ],
  "main": "dist/nodes/Netlify/Netlify.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/NetlifyApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Netlify/Netlify.node.js",
      "dist/nodes/Netlify/NetlifyTrigger.node.js"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build",
    "clean": "rimraf dist"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "^1.30.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "^1.30.0"
  },
  "engines": {
    "node": ">=18.10.0"
  }
}
